# Rename this file to .env and fill in your secrets

# Required: Discord bot token
DISCORD_TOKEN=your_discord_bot_token_here

# Required: PostgreSQL database connection URL
DATABASE_URL=postgresql://user:password@host:5432/database

# Required: OpenAI API key
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Override the model used (default: gpt-4.1-mini)
OPENAI_MODEL=gpt-4.1-mini

# Optional: Proxy for Discord connection (useful in regions where Discord is blocked)
# DISCORD_PROXY=socks5://127.0.0.1:7898
